@inherits UmbracoTemplatePage
@{
    Layout = "~/Views/Master.cshtml";
}
<section class="plain">
    <div>
        <!-- FORUM HEADER START -->
        <div class="utilities">
            @Umbraco.RenderMacro("Breadcrumb", new { linkToCurrent = "0" })
            <div class="clear"></div>
        </div>
        @Umbraco.RenderMacro("ProjectsProjectDetail")
    </div>
</section>

@section EndScripts {
    <script type="text/javascript">
        (function () {
            $(".fancybox")
                .fancybox({
                    openEffect: 'none',
                    closeEffect: 'none',
                    padding: 0
                });
        })();
    </script>
}